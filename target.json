{
    "llvm-target": "wasm32--wasi",
    "build-tags": [ "tinygo.wasm" ],
    "goos": "js",
    "goarch": "wasm",
    "linker": "wasm-ld",
    "libc": "wasi-libc",
    "cflags": [
      "--target=wasm32--wasi",
      "--sysroot={root}/lib/wasi-libc/sysroot",
      "-Oz"
    ],
    "ldflags": [
      "--allow-undefined",
      "--no-demangle",
      "--initial-memory=262144",
      "--max-memory=262144",
      "--stack-first",
      "--import-memory",
      "-zstack-size=8192",
      "--global-base=98304",
      "--strip-all",
      "--no-entry"
    ],
    "wasm-abi": "js"
  }
  